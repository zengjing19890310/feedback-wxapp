.menu-container .btn-icon image.main-icon{
  width:100rpx;
  height:85rpx;
  /* border:1px solid red; */
  vertical-align: middle;
  margin-top:50rpx;
}
.menu-container .btn-icon image.check-icon{
  width:50rpx;
  height:34rpx;
  /* border:1px solid yellow; */
  position: absolute;
  top:20rpx;
  right:20rpx;
}
.menu-container .btn-icon view{
  color:#999999;
}
.button-container{
  margin:100rpx 15%;
}
.button-container button{
  color:white;
  background-color:#448aff;
  height:80rpx;
  line-height: 80rpx;
  font-size:30rpx;
}
.button-container button.button-hover{
  background-color:#2770ea;
}