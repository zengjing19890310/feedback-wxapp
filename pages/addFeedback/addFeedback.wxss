.container.addFeedback{
  position: absolute;
  top:0;
  bottom:0;
  left:0;
  right:0;
  overflow: hidden;
  /* border:1rpx solid red; */
  font-size:0.8rem;
}
.container.addFeedback .header{
  text-align: center;
  margin:60rpx 0 65rpx 0;
  /* border:1px solid blue; */
}
.menu-container{
  /* border:1px solid red; */
  min-height:100rpx;
  margin:0 15%;
}
.menu-container::after{
  content:'';
  display: block;
  clear:both;
}
.menu-container .btn-icon{
  float:left;
  box-sizing: border-box;
  width:50%;
  height:200rpx;
  /* border:1px solid blueviolet; */
  text-align:center;
  position: relative;
}
